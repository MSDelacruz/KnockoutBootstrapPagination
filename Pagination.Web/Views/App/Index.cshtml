@{
    Layout = null;
}
<!doctype html>
<html class="no-js" lang="">
<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>KnockoutBootstrapPagination Demo</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon" href="@Url.Content("~/apple-touch-icon.png")">
    <link rel="stylesheet" href="@Url.Content("~/css/normalize.css")"/>

    <!-- bootstrap -->
    <link rel="stylesheet" href="//ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/css/bootstrap.css"  />

    <!-- custom -->
    <link rel="stylesheet" href="@Url.Content("~/css/main.css")">

    <script src="//ajax.aspnetcdn.com/ajax/modernizr/modernizr-2.6.2.js"></script>
    <script src="//ajax.aspnetcdn.com/ajax/jQuery/jquery-3.1.1.min.js"></script>
    <script src="//ajax.aspnetcdn.com/ajax/knockout/knockout-2.2.1.js"></script>
</head>
<body>
    <!--[if lte IE 9]>
        <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
    <![endif]-->
<!-- Add your site or application content here -->

    <div class="container">
        <div class="page-header">
            <hgroup>
                <h2>
                    Sample utilization of KnockoutBootstrap Pagination
                </h2>
                <h3>
                    Server data refresh with no caching
                </h3>
            </hgroup>
        </div>

        <div class="row">

                <nav aria-label="Page navigation">
                    <ul class="pagination" data-bind="template: {name: 'page-template', foreach: pagination.visiblePages }"></ul>
                </nav>


        </div>

        <div class="row">
            <div class="col-md-4">
                <table class="table table-strip table-responsive">
                    <tr>
                        <th>Id</th>
                        <th>Text</th>
                    </tr>
                    <tbody data-bind="template: { name: 'table-data', foreach: source } "></tbody>
                </table>
            </div>
        </div>
    </div>



    <script src="//ajax.aspnetcdn.com/ajax/bootstrap/3.3.7/bootstrap.min.js"></script>
    <script src="@Url.Content("~/js/pagination-viewmodel-ts.js")"></script>
    <script src="@Url.Content("~/js/demo-viewmodel-ts.js")"></script>

</body>
</html>
